<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>tryexcept</Title>
      <Shortcut>tryexcept</Shortcut>
      <Description>Code snippet for a try statement with Except pass</Description>
      <Author>Microsoft Corporation</Author>
      <SnippetTypes>
        <SnippetType>SurroundsWithStatement</SnippetType>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="Python"><![CDATA[
    try:
        $selected$
    except Exception:
        pass
$end$]]></Code>
    </Snippet>  
  </CodeSnippet>
</CodeSnippets>