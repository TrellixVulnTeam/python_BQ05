    "#This takes an input dir and merges pdfs\n",
    "from PyPDF2 import PdfFileMerger\n",
    "\n",
    "import funclib.iolib as iolib\n",
    "import funclib.baselib as baselib\n",
    "\n",
    "root = 'C:/Users/Graham/Downloads/sa'\n",
    "output_subfld = 'merged'\n",
    "output = '%s/%s' % (root, output_subfld)\n",
    "print('output fld:%s' % output)\n",
    "iolib.create_folder(folder_name=output)"
   ]
  },
